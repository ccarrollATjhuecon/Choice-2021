Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Function", " ", "to", " ", "construct", " ", "all", " ", "results", " ", 
    "using", " ", "variances", " ", "as", " ", "inputs"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"MakeResults", "[", 
     RowBox[{
     "\[ScriptP]Var_", ",", "\[CurlyTheta]Var_", ",", "ColorForPlots_"}], 
     "]"}], " ", ":=", " ", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "\[ScriptP]Std", ",", "\[CurlyTheta]Std", ",", 
        "\[Alpha]1FriedmanTheory", ",", "\[Alpha]0Est", ",", "\[Alpha]1Est"}],
        "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[ScriptP]Std", ",", "\[CurlyTheta]Std"}], "}"}], "=", 
        RowBox[{"{", 
         RowBox[{
          SuperscriptBox["\[ScriptP]Var", "0.5"], ",", 
          SuperscriptBox["\[CurlyTheta]Var", "0.5"]}], "}"}]}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Compute", " ", "standard", " ", "deviations", " ", "from", " ", 
         "variances"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Create", " ", "normally", " ", "distributed", " ", "shocks", " ", 
         "according", " ", "to", " ", "the", " ", "assumed", " ", 
         "distributions"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[CurlyTheta]Vec", "=", 
        RowBox[{"RandomVariate", "[", 
         RowBox[{
          RowBox[{"NormalDistribution", "[", 
           RowBox[{"0", ",", "\[CurlyTheta]Std"}], "]"}], ",", "NObs"}], 
         "]"}]}], ";", 
       RowBox[{"(*", " ", 
        RowBox[{"\[CurlyTheta]Vec", " ", "is", " ", "the", " ", "list", " ", 
         RowBox[{"(", "vector", ")"}], " ", "of", " ", "transitory", " ", 
         "shocks"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[ScriptP]Vec", "=", 
        RowBox[{"\[ScriptP]Mean", "+", 
         RowBox[{"RandomVariate", "[", 
          RowBox[{
           RowBox[{"NormalDistribution", "[", 
            RowBox[{"0", ",", "\[ScriptP]Std"}], "]"}], ",", "NObs"}], 
          "]"}]}]}], ";", 
       RowBox[{"(*", " ", 
        RowBox[{"\[ScriptP]Vec", " ", "is", " ", "the", " ", "list", " ", 
         RowBox[{"(", "vector", ")"}], " ", "of", " ", "permanent", " ", 
         "incomes"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "\[ScriptP]Mean", " ", "is", " ", "an", " ", "\"\<environment\>\"", 
         " ", "variable"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[ScriptY]Vec", "=", 
        RowBox[{"\[ScriptP]Vec", "+", "\[CurlyTheta]Vec"}]}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Actual", " ", "income", " ", "is", " ", "permanent", " ", "income", 
         " ", "plus", " ", "transitory"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"\[ScriptC]Vec", "=", "\[ScriptP]Vec"}], ";", 
       RowBox[{"(*", " ", 
        RowBox[{"Consumption", " ", "equals", " ", "permanent", " ", 
         RowBox[{"income", "!"}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"FitLine", "=", 
        RowBox[{"LinearModelFit", "[", 
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{"\[ScriptY]Vec", ",", "\[ScriptC]Vec"}], "}"}], "]"}], 
          ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "\[ScriptY]"}], "}"}], ",", "\[ScriptY]"}], 
         "]"}]}], ";", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Regress", " ", "\[ScriptC]", " ", "on", " ", "\[ScriptY]", " ", 
         "and", " ", "a", " ", "constant"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[Alpha]0Est", ",", "\[Alpha]1Est"}], "}"}], "=", 
        RowBox[{"FitLine", "[", "\"\<BestFitParameters\>\"", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<Predicted coefficient from Friedman formula vs estimated from \
data:\>\"", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\[Alpha]1FriedmanTheory", "=", 
            RowBox[{"\[ScriptP]Var", "/", 
             RowBox[{"(", 
              RowBox[{"\[ScriptP]Var", "+", "\[CurlyTheta]Var"}], ")"}]}]}], 
           ",", "\[Alpha]1Est"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
       
       RowBox[{"FitPlot", "=", 
        RowBox[{"Plot", "[", 
         RowBox[{
          RowBox[{"FitLine", "[", "\[ScriptY]", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[ScriptY]", ",", 
            RowBox[{"Min", "[", "\[ScriptY]Vec", "]"}], ",", 
            RowBox[{"Max", "[", "\[ScriptY]Vec", "]"}]}], "}"}], ",", 
          RowBox[{"PlotStyle", "\[Rule]", "ColorForPlots"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Degree45", "=", 
        RowBox[{"Plot", "[", 
         RowBox[{"\[ScriptY]", ",", 
          RowBox[{"{", 
           RowBox[{"\[ScriptY]", ",", "0", ",", 
            RowBox[{"Max", "[", "\[ScriptY]Vec", "]"}]}], "}"}], ",", 
          RowBox[{"PlotStyle", "\[Rule]", "Black"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"FitComparedToCEqualsY", "=", 
        RowBox[{"Show", "[", 
         RowBox[{"Degree45", ",", "FitPlot"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"DotPlot", "=", 
        RowBox[{"ListPlot", "[", 
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{"\[ScriptY]Vec", ",", "\[ScriptC]Vec"}], "}"}], "]"}], 
          ",", 
          RowBox[{"PlotStyle", "->", "ColorForPlots"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"{", 
         RowBox[{
         "\[Alpha]1Est", ",", "\[Alpha]1FriedmanTheory", ",", 
          "FitComparedToCEqualsY", ",", "DotPlot"}], "}"}], "]"}]}]}], 
     "]"}]}], ";"}]}]], "Input",
 InitializationCell->True],

